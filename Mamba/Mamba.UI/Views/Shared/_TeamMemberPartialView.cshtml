@model List<Team>
@{
    ViewData["Title"] = "_TeamMemberPartialView";
}


@foreach (Team team in Model)
{
    <div class="col-xl-3 col-lg-4 col-md-6" data-aos="fade-up">
        <div class="member">
            <div class="pic"><img src="~/uploads/team/@team.ImgUrl" class="img-fluid" alt=""></div>
            <div class="member-info">
                <h4>@team.FullName</h4>
                <span>@team.Profession?.Name</span>
                <div class="social">
                    <a href="@team.RedirectUrl"><i class="bi bi-twitter"></i></a>
                    <a href="@team.RedirectUrl"><i class="bi bi-facebook"></i></a>
                    <a href="@team.RedirectUrl"><i class="bi bi-instagram"></i></a>
                    <a href="@team.RedirectUrl"><i class="bi bi-linkedin"></i></a>
                </div>
            </div>
        </div>
    </div>
}